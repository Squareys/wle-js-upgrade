{
    "name": "wle-js-upgrade",
    "version": "1.0.1",
    "description": "Upgrade scripts for Wonderland Engine JavaScript projects",
    "license": "MIT",
    "type": "module",
    "main": "./dist/cli.mjs",
    "module": "./dist/cli.mjs",
    "scripts": {
        "build:ts": "tsc",
        "build:ts:watch": "tsc --watch",
        "test": "npm run build:ts && jasmine --config=test/jasmine.json",
        "prepublishOnly": "npm run build:ts",
        "pretty": "prettier --config ./.prettierrc.json --write ./"
    },
    "files": [
        "dist/*.js",
        "dist/*.mjs",
        "dist/*.d.ts",
        "data/*"
    ],
    "keywords": [
        "webxr",
        "wonderland-engine",
        "tools"
    ],
    "author": "Jonathan Hale",
    "bin": {
        "wle-js-upgrade": "./dist/cli.mjs"
    },
    "dependencies": {
        "prettier": "^2.8.7"
    },
    "devDependencies": {
        "@types/node": "^18.15.11",
        "@types/prettier": "^2.7.2",
        "jasmine": "^4.6.0",
        "prettier": "^2.8.7",
        "typescript": "^5.0.3"
    }
}
